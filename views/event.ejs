<h1>Events</h1>

<div class="row">
  <div class="col-sm-8">
    <div class="card">
      <div class="card-body">
        <!-- Makes POST request to /login route -->
        <form action="/api/admin/newEvent" method="POST">
          <div class="form-group">
            <label for="email">Event Title</label>
            <input type="string" class="form-control" name="eventName">
          </div>
          <div class="form-group">
            <label for="password">EventDesc</label>
            <input type="string" class="form-control" name="eventDesc">
          </div>
          <div class="form-group">
            <label for="password">Event link</label>
            <input type="string" class="form-control" name="eventLink">
          </div>
          <button type="submit" class="btn btn-dark">Submit</button>
        </form>

      </div>
    </div>
  </div>